<!DOCTYPE html>
<html>
    <head>
        <title>A chess test room</title>
        <link rel="stylesheet" href="/stylesheets/board.css"></link>
    </head>
    <body>

        <div id="board-container">
            <% for (var i = 0; i < 8; i++) { %>
                <div class="board-row">
                    <% for (var j = 0; j < 8; j++) { %>
                    <div class="board-cell">A1</div>
                    <% } %>
                </div>
            <% } %>
        </div>

        <div id="command-input">
            <input id="command" class="one-line-input" type="text">
            <button class="send-button">Go!</button>
        </div>

    <!-- Script section -->
    <script src='http://code.jquery.com/jquery-2.1.3.min.js'></script>
     <script type='text/javascript' src='/javascripts/sendMove.js'></script>
    <!-- /Script section -->
    </body>
</html>